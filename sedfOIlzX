document.addEventListener('DOMContentLoaded', function() {
  var botonesLeerMas = document.querySelectorAll('.leer-mas');
  var contenidosOcultos = document.querySelectorAll('.contenido-oculto');

  for (var i = 0; i < botonesLeerMas.length; i++) {
    botonesLeerMas[i].addEventListener('click', function() {
      var contenidoOculto = this.nextElementSibling;
      contenidoOculto.classList.toggle('mostrar');

      var textoBoton = this.textContent.trim().replace(/^[\+\-]\s*/, '');
      var nuevoTextoBoton = textoBoton === 'Seguir leyendo' ? '- Leer menos' : '+ Seguir leyendo';
      this.textContent = nuevoTextoBoton;
    });
  }
});
